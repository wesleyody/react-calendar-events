{
  "name": "react-calendar-events",
  "version": "2.4.1",
  "description": "React calendar for displaying events",
  "author": "Wesley <wesleyody@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wesleyody/react-calendar-events.git"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "lib/",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "scheduler",
    "react-component",
    "react",
    "calendar",
    "events",
    "full calendar"
  ],
  "scripts": {
    "babel": "babel ./src --out-dir ./lib",
    "build": "npm run babel && npm run sass",
    "sass": "cpx \"./src/**/*.scss\" ./lib",
    "lint": "eslint src",
    "release": "release",
    "prepublish": "npm run build",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.js": "eslint",
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "peerDependencies": {
    "react": "16.x || 17.x || 18.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-modules-commonjs": "^7.20.11",
    "@babel/plugin-transform-react-jsx": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "babel-loader": "^9.1.2",
    "cpx": "^1.5.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.31.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "lint-staged": "^13.1.0",
    "mini-css-extract-plugin": "^2.7.2",
    "node-sass": "^6.0.1",
    "sass-loader": "^13.2.0",
    "webpack": "^5.75.0"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "date-arithmetic": "^4.1.0",
    "dom-helpers": "^5.2.1",
    "invariant": "^2.1.0",
    "lodash": "^4.17.21",
    "prop-types": "^15.5.8",
    "react-prop-types": "^0.4.0",
    "uncontrollable": "^7.2.1"
